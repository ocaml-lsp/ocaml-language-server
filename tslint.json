{
  "extends": "tslint:latest",
  "rules": {
    "arrow-parens": false,
    "curly": false,
    "max-line-length": [
      false
    ],
    "no-conditional-assignment": false,
    "no-empty-interface": false,
    "no-namespace": false,
    "no-shadowed-variable": false,
    "space-before-function-paren": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  }
}
